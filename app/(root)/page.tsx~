const Home = () => {
    return (
        <div>
            {/* 用户已登录时显示 Home 页面 */}
            <SignedIn>
                <p>Home</p>

            </SignedIn>

            {/* 用户未登录时，跳转到 SignIn 页面 */}
            <SignedOut>
                <RedirectToSignIn />
            </SignedOut>
        </div>
    );
};

export default Home;
